<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <link rel='stylesheet' href='css/style.css' />
    <link
      href='https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css'
      rel='stylesheet'
      integrity='sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x'
      crossorigin='anonymous'
    />
    <title>Document</title>

  </head>
  <body>
    <h2>TEST TRANSACTION FORM</h2>
    <form action='/createTestTransaction' method='POST'>
      <div class='mb-3'>
        <label class='form-label'>ID(PRIMARY ID)</label>
        <input type='text' class='form-control' name='id' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Name</label>
        <input type='text' class='form-control' name='name' />
      </div>
      <div class='mb-3 form-check'>
        <input type='checkbox' class='form-check-input' name='status' />
        <label class='form-check-label'>Status</label>
      </div>
      <div class='mb-3'>
        <label class='form-label'>Test Date</label>
        <input type='date' class='form-control' name='test_date' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Valid Until</label>
        <input type='date' class='form-control' name='valid_until' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Evidence</label>
        <input type='text' class='form-control' name='evidence' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Tested By</label>
        <input type='text' class='form-control' name='tested_by' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Created By:</label>
        <input type='text' class='form-control' name='created_by' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Modified By:</label>
        <input type='text' class='form-control' name='modified_by' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Created Date</label>
        <input type='date' class='form-control' name='created_date' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Modified Date</label>
        <input type='date' class='form-control' name='modified_date' />
      </div>

      <div class='mb-3'>
        <label class='form-label'>Test File Path </label>
        <input type='text' class='form-control' name='test_file_path' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Evidence File Path </label>
        <input type='text' class='form-control' name='evidence_file_path' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Test Bucket Name </label>
        <input type='text' class='form-control' name='test_bucket_name' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Test Object Key </label>
        <input type='text' class='form-control' name='test_object_key' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Evidence Bucket Name </label>
        <input type='text' class='form-control' name='evidence_bucket_name' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Evidence Bucket Name </label>
        <input type='text' class='form-control' name='evidence_object_key' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Program Id </label>
        <input type='text' class='form-control' name='program_id' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Program Certificate Id </label>
        <input type='text' class='form-control' name='program_certificate_id' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Program Controls Id </label>
        <input type='text' class='form-control' name='program_controls_id' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Program Policy Id </label>
        <input type='text' class='form-control' name='program_policy_id' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>System Id </label>
        <input type='text' class='form-control' name='system_id' />
      </div>

      <div class='mb-3'>
        <label class='form-label'>Test Id</label>
        <input type='text' class='form-control' name='test_id' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Control Id</label>
        <input type='text' class='form-control' name='controls_id' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Customer Id</label>
        <input type='text' class='form-control' name='customer_id' />
      </div>
      <div class='mb-3'>
        <label class='form-label'>Policy Id</label>
        <input type='text' class='form-control' name='policy_id' />
      </div>

      <button type='submit' class='btn btn-primary'>Submit</button>
    </form>
  </body>
</html>